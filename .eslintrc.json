{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "import/no-extraneous-dependencies":"off",
        "no-underscore-dangle":"off",
        "camelcase":"off",
        "class-methods-use-this":"off",
        "no-unused-vars": "off",
        "linebreak-style": "off"
    }
}
